<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Pattern for the v1 month and year string (localised) -->
    <string name="mdtp_date_v1_monthyear">MMMM yyyy</string>
    <!-- Pattern for the v2 day month and year string (localised) -->
    <string name="mdtp_date_v2_daymonthyear">EEE, MMM dd</string>

    <!-- Label for date or time [CHAR LIMIT=30] -->
    <string name="mdtp_done_label">เลือก</string>
    <string name="mdtp_ok">ตกลง</string>
    <string name="mdtp_cancel">ยกเลิก</string>
    <!--
         Content description for the hour selector in the time picker, which displays
         selectable hours of the day along the inside edge of a circle, as in an analog clock.
         [CHAR LIMIT=50]
    -->
    <string name="mdtp_hour_picker_description">เลือกชั่วโมง</string>
    <!--
         Content description for the minute selector in the time picker, which displays
         selectable five-minute intervals along the inside edge of a circle, as in an analog clock.
         [CHAR LIMIT=50]
    -->
    <string name="mdtp_minute_picker_description">เลือกนาที</string>
    <!--
         Content description for the second selector in the time picker, which displays
         selectable five-second intervals along the inside edge of a circle, as in an analog clock.
         [CHAR LIMIT=50]
    -->
    <string name="mdtp_second_picker_description">เลือกวินาที</string>
    <!-- Accessibility announcement for hour circular picker [CHAR LIMIT=NONE] -->
    <string name="mdtp_select_hours">เลือกชั่วโมง</string>
    <!-- Accessibility announcement for minute circular picker [CHAR LIMIT=NONE] -->
    <string name="mdtp_select_minutes">เลือกนาที</string>
    <!-- Accessibility announcement for second circular picker [CHAR LIMIT=NONE] -->
    <string name="mdtp_select_seconds">เลือกวินาที</string>

    <string name="mdtp_date">วัน</string>
    <string name="mdtp_time">เวลา</string>

    <!--
        Content description for the month and day selector in the date picker, which displays
        a selectable grid of days laid out by month.
        [CHAR LIMIT=50]
     -->
    <string name="mdtp_day_picker_description">ตารางเดือน</string>
    <!--
        Content description for the year selector in the date picker, which displays
        a scrolling, vertical list of years.
        [CHAR LIMIT=50]
     -->
    <string name="mdtp_year_picker_description">รายการปี</string>
    <!-- Accessibility announcement for the day picker [CHAR LIMIT=NONE] -->
    <string name="mdtp_select_day">เลือกเดือนและวัน</string>
    <!-- Accessibility announcement for the year picker [CHAR LIMIT=NONE] -->
    <string name="mdtp_select_year">เลือกปี</string>
    <!-- Accessibility description for the item that is currently selected. -->
    <string name="mdtp_item_is_selected"><xliff:g id="item" example="2013">%1$s</xliff:g> เลือก</string>
    <!-- Accessibility announcement when a number that had been typed in is deleted [CHAR_LIMIT=NONE] -->
    <string name="mdtp_deleted_key"><xliff:g id="key" example="4">%1$s</xliff:g> ลบ</string>
    <!-- Accessibility announcement for the month arrows in the day picker -->
    <string name="mdtp_next_month_arrow_description">เดือนถัดไป</string>
    <string name="mdtp_previous_month_arrow_description">เดือนก่อนหน้า</string>

    <!-- DO NOT TRANSLATE -->
    <string name="mdtp_time_placeholder" translatable="false">00</string>
    <!-- DO NOT TRANSLATE -->
    <string name="mdtp_time_separator" translatable="false">:</string>

    <!-- DO NOT TRANSLATE -->
    <string name="mdtp_radial_numbers_typeface" translatable="false">sans-serif</string>
    <!-- DO NOT TRANSLATE -->
    <string name="mdtp_sans_serif" translatable="false">sans-serif</string>

    <!-- DO NOT TRANSLATE -->
    <string name="mdtp_day_of_week_label_typeface" translatable="false">sans-serif</string>

    <string name="mdtp_am">ก่อนเที่ยง</string>
    <string name="mdtp_pm">หลังเที่ยง</string>


    <string name="mdtp_buddhist">พ.ศ.</string>

</resources>
